---
layout: layouts/base.njk
section: artists
---

<!-- Start Page Banner -->
<div class="artist-banner-area">
    <div class="container">
        <div class="page-banner-content">
            <h2 class="color-kenny-blauw">Programma</h2>
        </div>
    </div>
</div>
<!-- End Page Banner -->

{% set artistlist = collections.artist %}

<!-- Start Speakers Area -->
<div class="speakers-area ptb-100">
    <div class="container">
        <div class="row justify-content-center">

            {% for artist in artistlist %}
                <div class="col-lg-8 col-md-6">
                    <a href="#" class="single-speakers-box" style="background-image: url('/static/img/mmai.png');">
                        <div class="speakers-content">
                            <div class="top-content">
                                <h3>
                                    {{ artist.data.name }}
                                </h3>
                            </div>
                        </div>
                    </a>
                </div>
            {% endfor %}

